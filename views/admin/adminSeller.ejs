<div class="" style="padding-left: 5%; padding-right: 5%;">



<% users.forEach(user => { %>
<p style="font-family: 'Manrope', sans-serif;">  <%=user.username;%> </p>

<% if(user.isAdmin == true){ %>
 <p style="font-family: 'Manrope', sans-serif;">Admin Account</p>
<% } else { %>


<a style="background-color: black;
  color: white;
  padding: 4px 24px;
  text-align: center;
  text-decoration: none;
  display: inline-block;
  font-size: 14px;
  margin: 4px 2px;
  cursor: pointer;
  text-decoration: none;
  text-shadow: 0 0 2px #1F51FF, 0 0 3px #1F51FF;
  box-shadow: 0 0 2px #1F51FF, 0 0 3px #1F51FF;
  border-radius: 5%;
  font-family: 'Manrope', sans-serif;" href="/adminSeller/<%= user._id %>">Edit</a>
<form class="" action="/adminSeller/<%= user._id %>?_method=DELETE" method="post">
<button style="background-color: black;
  color: white;
  padding: 4px 24px;
  text-align: center;
  text-decoration: none;
  display: inline-block;
  font-size: 14px;
  margin: 4px 2px;
  cursor: pointer;
  text-decoration: none;
  text-shadow: 0 0 2px #1F51FF, 0 0 3px #1F51FF;
  box-shadow: 0 0 2px #1F51FF, 0 0 3px #1F51FF;
  border-radius: 5%;
  font-family: 'Manrope', sans-serif;">Delete</button>
</form>

<% } %>

<%});%>


</div>
